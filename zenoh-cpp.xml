<manifest>
    <remote name="up" fetch="git@github.com:eclipse-uprotocol" />
    <remote name="zenoh" fetch="git@github.com:eclipse-zenoh" />
    <default remote="up" revision="main" />

    <project name="zenoh-c" path="zenoh-c" remote="zenoh" /> 
    <project name="up-core-api" path="up-core-api" remote="up" />
    <project name="up-cpp" path="up-cpp" remote="up" />
    <project name="up-client-zenoh-cpp" remote="up" />
</manifest>